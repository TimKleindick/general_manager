# Docker Compose defaults for project_management

# Web port on host
PM_HTTP_PORT=8000

# Meilisearch master key used by both web and meilisearch services
MEILISEARCH_API_KEY=project-management-master-key

# Optional: Gemini key for MCP AI chat endpoint (/ai/chat)
GEMINI_API_KEY=

# Optional: seed demo data on container startup
PM_SEED_ON_START=false

# PostgreSQL
POSTGRES_DB=project_management
POSTGRES_USER=project_management
POSTGRES_PASSWORD=project_management
POSTGRES_CONN_MAX_AGE=60
