{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum by (status) (rate(django_http_responses_total_by_status_total{job=\"orl-web\"}[5m]) or rate(django_http_responses_total_by_status{job=\"orl-web\"}[5m]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Responses by Status",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(django_http_requests_latency_including_middlewares_seconds_bucket{job=\"orl-web\"}[5m]) or rate(django_http_request_duration_seconds_bucket{job=\"orl-web\"}[5m]) or rate(django_http_request_latency_seconds_bucket{job=\"orl-web\"}[5m])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "title": "Request Duration p95",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "req/s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 7
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum by (view, method) (rate(django_http_requests_total_by_view_transport_method_total{job=\"orl-web\"}[5m]) or rate(django_http_requests_total_by_view_method_total{job=\"orl-web\"}[5m]) or rate(django_http_requests_total_by_view_method{job=\"orl-web\"}[5m]))",
          "legendFormat": "{{view}} {{method}}",
          "refId": "A"
        }
      ],
      "title": "Requests by View",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "cores"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 7
      },
      "id": 4,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=\"orl-web\"}[5m])",
          "legendFormat": "cpu",
          "refId": "A"
        }
      ],
      "title": "App CPU (rate)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "id": 5,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job=\"orl-web\"}",
          "legendFormat": "rss",
          "refId": "A"
        }
      ],
      "title": "App Memory (RSS)",
      "type": "timeseries"
    },
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": {
          "unit": "collections/s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 7,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "id": 6,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(python_gc_collections_total{job=\"orl-web\"}[5m])",
